#define CASE=14
#ifexists "../case.glm"
#define DIR=..
#endif

module pypower
{
#ifdef DIR
    controllers_path "${DIR}";
#endif
    controllers "controllers";
    stop_on_failure FALSE;
}

object pypower.load
{
    parent pp_bus_2;
    Vn 12.5 kV;
    status ONLINE;
    controller "load_control";
}

object pypower.powerplant
{
    parent pp_bus_2;
    status ONLINE;
    controller "powerplant_control";
}

#include "${DIR:-.}/case.glm"
